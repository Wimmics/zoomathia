<http://www.zoomathia.com/Acut.sp./10/>
      a       schema:Chapter ;
      schema:hasPart <http://www.zoomathia.com/Acut.sp./10//19> , <http://www.zoomathia.com/Acut.sp./10//17> , <http://www.zoomathia.com/Acut.sp./10//18> , <http://www.zoomathia.com/Acut.sp./10//15> , <http://www.zoomathia.com/Acut.sp./10//16> , <http://www.zoomathia.com/Acut.sp./10//13> , <http://www.zoomathia.com/Acut.sp./10//14> , <http://www.zoomathia.com/Acut.sp./10//1> , <http://www.zoomathia.com/Acut.sp./10//3> , <http://www.zoomathia.com/Acut.sp./10//2> , <http://www.zoomathia.com/Acut.sp./10//5> , <http://www.zoomathia.com/Acut.sp./10//4> , <http://www.zoomathia.com/Acut.sp./10//7> , <http://www.zoomathia.com/Acut.sp./10//6> , <http://www.zoomathia.com/Acut.sp./10//9> , <http://www.zoomathia.com/Acut.sp./10//22> , <http://www.zoomathia.com/Acut.sp./10//11> , <http://www.zoomathia.com/Acut.sp./10//12> , <http://www.zoomathia.com/Acut.sp./10//23> , <http://www.zoomathia.com/Acut.sp./10//8> , <http://www.zoomathia.com/Acut.sp./10//20> , <http://www.zoomathia.com/Acut.sp./10//10> , <http://www.zoomathia.com/Acut.sp./10//21> ;
